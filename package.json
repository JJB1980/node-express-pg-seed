    "jsonwebtoken": "^8.2.0",
    "multer": "^1.3.0",
    "nodemailer": "^4.6.4",
    "nodemon": "^1.17.5",
    "password-hash": "^1.2.2",
    "pg": "^7.4.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
